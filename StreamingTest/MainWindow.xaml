<Window x:Class="StreamingTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StreamingTest"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <Button Click="ButtonBase_OnClick" Height="100">TMP</Button>
        <Label Content="title"></Label>
        <Label Name="title"></Label>
        <Label Content="album"></Label>
        <Label Name="album"></Label>
        <Label Content="artist"></Label>
        <Label Name="artist"></Label>
        <Label Content="composer"></Label>
        <Label Name="composer"></Label>
        <Label Content="group"></Label>
        <Label Name="group"></Label>
    </StackPanel>
</Window>
